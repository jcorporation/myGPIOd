#Example mygpiod.conf
#
#myGPIOd @CPACK_PACKAGE_VERSION_MAJOR@.@CPACK_PACKAGE_VERSION_MINOR@.@CPACK_PACKAGE_VERSION_PATCH@
#https://github.com/jcorporation/myGPIOd
#
#logging
#0 = emerg, 1 = alert, 2 = crit, 3 = error, 4 = warn, 5 = notice, 6 = info, 7 = debug
loglevel=5
syslog=false

#gpio chip number
chip=0

#trigger events
edge=falling
#edge=rising
#edge=both
active_low=true
#active_low=false
bias=as-is
#bias=disable
#bias=pull-down
#bias=pull-up

#gpio,edge,script
#3,falling,echo "test 3"
